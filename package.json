{
  "name": "smallfish_proxy",
  "version": "1.0.0",
  "scripts": {
    "start": "scripts/run.sh dev --not-testing",
    "stop": "scripts/destroy.sh",
    "test": "scripts/run.sh dev --testing && sleep 1 && NODE_ENV=test ./node_modules/mocha/bin/mocha --reporter list test/*.js; npm stop",
    "test:dev": "NODE_ENV=development ./node_modules/mocha/bin/mocha --reporter list test/*.js",
    "test:stage": "NODE_ENV=stage ./node_modules/mocha/bin/mocha --reporter list test/*.js",
    "test:prod": "NODE_ENV=production ./node_modules/mocha/bin/mocha --reporter list test/*.js"
  },
  "devDependencies": {
    "aws-sdk": "^2.6.15",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "glob": "^7.1.1",
    "mocha": "^3.1.2",
    "supertest": "^2.0.1",
    "supertest-promised": "^1.0.0",
    "yargs": "^6.3.0"
  },
  "dependencies": {
    "global": "^4.3.1"
  }
}
